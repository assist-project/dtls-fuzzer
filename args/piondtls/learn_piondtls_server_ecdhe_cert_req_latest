# for versions from 2.0.9 onward
state-fuzzer-server
-connect 
localhost:${sut.port}
-runWait 
100
-rstWait
100
-alphabet
${fuzzer.dir}/${alphabets.servers}/piondtls_server_ecdhe_cert.xml
-timeout  
100  
-randLength
10
-queries
20000
-protocol 
DTLS12 
-equivalenceAlgorithms
RANDOM_WP_METHOD,WP_METHOD
-output 
output/piondtls-${piondtls.version}_server_ecdhe_cert_req
-cmd 
"${suts.dir}/piondtls-${piondtls.version}/dtls-clientserver -role server -port ${sut.port} -key experiments/keystore/piondtls_ec_secp256r1_key.pem -cert experiments/keystore/piondtls_ec_secp256r1_cert.pem -cipherSuite TLS_ECDHE_RSA_WITH_AES_128_GCM_SHA256 -clientAuth NEEDED -trustCert ${fuzzer.dir}/experiments/keystore/rsa2048_cert.pem -serverName tls-attacker.de"
-termCmd
"/bin/bash  ${fuzzer.dir}/experiments/scripts/stop_proc_at_port.sh ${sut.port} server"
-depth
1
-repeatingOutputs
HELLO_VERIFY_REQUEST
-dontCacheTests

