# for versions from 2.0.9 onward
state-fuzzer-client

-port
${sul.port}
-startWait
0
-alphabet
${alphabets.clients}/piondtls_ecdhe_cert.xml
-responseWait
100
-randLength
10
-equivalenceQueryBound
20000
-protocol
DTLS12
-equivalenceAlgorithms
RANDOM_WP_METHOD,WP_METHOD
-output
output/piondtls-${piondtls.version}_client_ecdhe_cert
-cmd
${suts.dir}/piondtls-${piondtls.version}/dtls-clientserver -role client -port ${sul.port} -key experiments/keystore/piondtls_ec_secp256r1_key.pem -cert experiments/keystore/piondtls_ec_secp256r1_cert.pem  -cipherSuite TLS_ECDHE_RSA_WITH_AES_128_GCM_SHA256 -clientAuth NEEDED -trustCert ${fuzzer.dir}/experiments/keystore/rsa2048_cert.pem -serverName tls-attacker.de
-depth
1
-dontCacheTests
