state-fuzzer-server

# SUT config
-protocol
DTLS12
-connect
localhost:${sut.port}
-startWait
200
-responseWait
20
-cmd
${fuzzer.dir}/suts/openssl-${openssl.version}/apps/openssl s_server -psk 1234 -key ${fuzzer.dir}/experiments/keystore/rsa2048_key.pem -cert ${fuzzer.dir}/experiments/keystore/rsa2048_cert.pem -CAfile ${fuzzer.dir}/experiments/keystore/rsa2048_cert.pem -accept ${sut.port} -dtls1_2 -mtu 5000

# learning config
-randLength
10
-queries
20000
-equivalenceAlgorithms
RANDOM_WP_METHOD,WP_METHOD
-alphabet
${alphabets.servers}/dhe_ecdhe_psk_rsa_cert.xml
-output
output/openssl-${openssl.version}_server_all_cert_none
