state-fuzzer-server

# SUT config
-protocol 
DTLS12 
-connect 
localhost:${sut.port}
-runWait 
100 
-timeout  
100 
-cmd 
"${fuzzer.dir}/suts/gnutls-${gnutls.version}/src/gnutls-serv  --udp --x509keyfile ${fuzzer.dir}/experiments/keystore/rsa2048_key.pem --x509certfile ${fuzzer.dir}/experiments/keystore/rsa2048_cert.pem --x509cafile ${fuzzer.dir}/experiments/keystore/rsa2048_cert.pem --pskpasswd ${fuzzer.dir}/experiments/keystore/keys.psk --priority NORMAL:+PSK:+SRP  --disable-client-cert --mtu 1500 -p ${sut.port}"

# learning config
-alphabet 
${alphabets.servers}/gnutls_dhe_ecdhe_psk_rsa_cert.xml 
-randLength
10
-queries
20000
-equivalenceAlgorithms 
RANDOM_WP_METHOD,WP_METHOD
-output
output/gnutls-${gnutls.version}_all_cert_none_rwalk
-ceReruns
3
