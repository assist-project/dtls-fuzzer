<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>CUSTOM-format</actionName>
        <displayName>format</displayName>
        <goals>
            <goal>initialize</goal>
            <goal>formatter:format</goal>
        </goals>
    </action>
    <action>
        <actionName>CUSTOM-license</actionName>
        <displayName>license</displayName>
        <goals>
            <goal>initialize</goal>
            <goal>license:format</goal>
        </goals>
    </action>
    <action>
        <actionName>build</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>install</goal>
        </goals>
        <properties>
                <skipITs>true</skipITs>
            </properties>
    </action>
    <action>
        <actionName>rebuild</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>clean</goal>
            <goal>install</goal>
        </goals>
        <properties>
                <skipITs>true</skipITs>
            </properties>
    </action>
    <action>
            <actionName>CUSTOM-site</actionName>
            <displayName>site</displayName>
            <goals>
                <goal>site</goal>
            </goals>
        </action>
</actions>
