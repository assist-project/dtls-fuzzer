<!-- DTLS alphabet comprising the messages necessary to perform an ECDHE handshakes ciphersuite. -->
<alphabet>
    <ClientHelloInput suite="TLS_ECDHE_ECDSA_WITH_AES_128_CBC_SHA256" name="ECDH_CLIENT_HELLO" extendedWait="300"/>
    <ClientKeyExchangeInput name="ECDH_CLIENT_KEY_EXCHANGE" algorithm="ECDH"/>
    <ChangeCipherSpecInput/>
    <FinishedInput name="FINISHED" extendedWait="300"/>
    <GenericTlsInput name="APPLICATION">
        <Application>
            <data>
                <byteArrayExplicitValueModification>
                        <!--HEX(This is a hello message!)-->
                        <explicitValue>0x5468697320697320612068656c6c6f206d65737361676521</explicitValue>
                </byteArrayExplicitValueModification>
            </data>
        </Application>
    </GenericTlsInput>
    <GenericTlsInput name="CERTIFICATE">
        <Certificate/>
    </GenericTlsInput>
    <GenericTlsInput name="EMPTY_CERTIFICATE">
        <Certificate>
            <certificatesList>
            </certificatesList>
        </Certificate>
    </GenericTlsInput>
    <GenericTlsInput name="CERTIFICATE_VERIFY">
        <CertificateVerify/>
    </GenericTlsInput>
    <AlertInput name="Alert(WARNING,CLOSE_NOTIFY)" level="WARNING" description="CLOSE_NOTIFY"/>
    <AlertInput name="Alert(FATAL,UNEXPECTED_MESSAGE)" level="FATAL" description="UNEXPECTED_MESSAGE"/>
</alphabet>
