    private static List<TlsWord> getShortWords() {
        List<TlsWord> wordList = new LinkedList<>();
//        wordList.add(new GenericMessageWord(new CertificateMessage()));
        wordList.add(new ClientHelloWord(CipherSuite.TLS_RSA_WITH_AES_128_CBC_SHA));
//        wordList.add(new GenericMessageWord(new AlertMessage()));
        wordList.add(new GenericMessageWord(new ApplicationMessage()));
        wordList.add(new ChangeCipherSpecWord());
        wordList.add(new GenericMessageWord(new RSAClientKeyExchangeMessage()));
        wordList.add(new FinishedWord());
//        wordList.add(new GenericMessageWord(new HeartbeatMessage()));
        return wordList;
    }